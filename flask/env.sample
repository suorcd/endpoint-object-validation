# Copy to .env and customize for your cluster

# Docker image name for Flask app
IMAGE_NAME=gitea.ser.ink/ser/eov-flask:latest

# k3s hosts
SERVER_NODE_HOST=192.0.2.10
WORKER_NODE_HOST=192.0.2.11
NODE_IPS="192.0.2.10 192.0.2.11"
INGRESS_HOST=192.0.2.10

# Optional: tailscale DNS name used for access
TAILSCALE_HOSTNAME=eov-flask.tailnet-name.ts.net

# Optional: SSH user for k3s nodes
SSH_USER=root
